@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
      crossorigin="anonymous">

<div id="app">
    <div class="justify-content-center align-items-center text-center">
        <div class="container text-center">
            <div class="row text-center pb-2">
                <div class="col-12 pt-3">
                    <h1 class="display-4 text-center">Search for Azure Documents</h1>
                </div>
            </div>
            <div class="row mb-2">
                <div class="col-12 d-flex justify-content-center">
                    <div class="col-12 d-flex justify-content-center" id="searchBox"></div>
                    <div id="navbar" class="navbar-collapse collapse">
                    </div>
                </div>
            </div>
            <div class="row">
                <hr style="        width: 100% !important;
        border: 0;
        height: 2px;
        background-image: linear-gradient(to right, rgba(0, 0, 0, 0.75),rgba(0, 0, 0, 0.75));" />
            </div>
            <div class="row">
                <div class="col-12 results_section">
                    <div id="results" class="row placeholders">
                    </div>
                    <div id="pager" class="row">
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                <div id="facetPanel" class="sidebar">
                    <div id="clearFilters"></div>
                    <ul class="nav nav-sidebar">
                        <div className="panel panel-primary behclick-panel">

                            <li>
                                <div id="metadata_storage_file_extensionFacet">
                                </div>
                            </li>
                            <li>
                                <div id="keyphrasesFacet">
                                </div>
                            </li>
                            <li>
                                <div id="imageTagsFacet">
                                </div>
                            </li>
                        </div>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>